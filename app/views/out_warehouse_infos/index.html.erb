<p id="notice"><%= notice %></p>

<h1>Out Warehouse Infos</h1>

<table>
  <thead>
    <tr>
      <th>Out no</th>
      <th>Out address</th>
      <th>Buyer phone</th>
      <th>Out time</th>
      <th>Finish</th>
      <th>Buyer name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @out_warehouse_infos.each do |out_warehouse_info| %>
      <tr>
        <td><%= out_warehouse_info.out_no %></td>
        <td><%= out_warehouse_info.out_address %></td>
        <td><%= out_warehouse_info.buyer_phone %></td>
        <td><%= out_warehouse_info.out_time %></td>
        <td><%= out_warehouse_info.finish %></td>
        <td><%= out_warehouse_info.buyer_name %></td>
        <td><%= link_to 'Show', out_warehouse_info %></td>
        <td><%= link_to 'Edit', edit_out_warehouse_info_path(out_warehouse_info) %></td>
        <td><%= link_to 'Destroy', out_warehouse_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Out Warehouse Info', new_out_warehouse_info_path %>
